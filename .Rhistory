calculate_chi_squared_p_values <- function(data, class_column) {
p_values <- sapply(names(data), function(column) {
if (column != class_column) {
chi_squared_result <- chisq.test(data[[column]], data[[class_column]])
return(chi_squared_result$p.value)
}
return(NULL)
})
names(p_values) <- names(data)[-which(names(data) == class_column)]
return(p_values)
}
